{
  "name": "rc-input-number",
  "version": "7.4.0",
  "description": "React input-number component",
  "keywords": [
    "react",
    "react-component",
    "react-input-number",
    "input-number"
  ],
  "homepage": "https://github.com/react-component/input-number",
  "bugs": {
    "url": "http://github.com/react-component/input-number/issues"
  },
  "repository": {
