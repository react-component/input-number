    "glob": "^7.1.6",
    "less": "^3.12.2",
    "np": "^7.2.0",
    "rc-tooltip": "^5.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.7",
    "ts-node": "^10.9.1",
